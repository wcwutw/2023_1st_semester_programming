class List{
	field int data;
	field List next;
	constructor List New(int car, List cdr) {
		let data=car;
		let next=cdr;
		return this;
	}
	method void push_front(int val) {
		List temp=next;
		next=List.new(val, temp);
		return;
	}
	method void dispose() {
		if(~(next=null)) {
			do next.dispose();
		}
		do Memory.deAlloc(this);
		return;
	}
}
